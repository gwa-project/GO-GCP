# Environment Variables Template s
# Copy this file to .env and fill in your values

# MongoDB Configuration
MONGOSTRING=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# Security Keys
PRKEY=your-private-key-here

# Application Configuration
ENVIRONMENT=development
PORT=8080

# Google Cloud Configuration (for local development)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json